{
  "name": "quizzes-tutor-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve --fix",
    "build": "NODE_ENV=production vue-cli-service build --mode production",
    "build:dev": "NODE_ENV=development vue-cli-service build --mode development",
    "lint": "vue-cli-service lint",
    "prettier": "prettier --single-quote --write \"**/**/*.{ts,js,css,scss,vue}\"",
    "codecov": "codecov",
    "cypress": "npx cypress open",
    "test:e2e": "npx cypress run",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "~6.1.1",
    "axios": "~0.27.2",
    "glob-all": "^3.3.0",
    "material-design-icons-iconfont": "~6.7.0",
    "pg": "~8.7.3",
    "plotly.js-cartesian-dist-min": "~2.12.1",
    "qrcode": "~1.5.0",
    "sanitize-html": "~2.7.0",
    "showdown": "~2.1.0",
    "typeface-roboto": "~1.1.13",
    "vue": "~2.6.14",
    "vue-class-component": "~7.2.6",
    "vue-codemirror": "~4.0.6",
    "vue-ctk-date-time-picker": "~2.5.0",
    "vue-property-decorator": "~9.1.2",
    "vue-qrcode": "~0.4.0",
    "vue-qrcode-reader": "~2.3.18",
    "vue-router": "~3.5.3",
    "vue-underscore": "~0.1.4",
    "vuedraggable": "~2.24.3",
    "vuetify": "~2.6.6",
    "vuex": "~3.6.2"
  },
  "devDependencies": {
    "@mdi/font": "~5.9.55",
    "@types/codemirror": "~0.0.109",
    "@types/jest": "~27.4.1",
    "@types/plotly.js-dist-min": "~2.3.0",
    "@types/qrcode": "~1.4.2",
    "@typescript-eslint/eslint-plugin": "~5.20.0",
    "@typescript-eslint/parser": "~5.20.0",
    "@vue/cli-plugin-babel": "~5.0.4",
    "@vue/cli-plugin-eslint": "~5.0.4",
    "@vue/cli-plugin-pwa": "~5.0.4",
    "@vue/cli-plugin-router": "~5.0.4",
    "@vue/cli-plugin-typescript": "~5.0.4",
    "@vue/cli-plugin-unit-jest": "~5.0.4",
    "@vue/cli-plugin-vuex": "~5.0.4",
    "@vue/cli-service": "~5.0.4",
    "@vue/eslint-config-prettier": "~7.0.0",
    "@vue/eslint-config-typescript": "~10.0.0",
    "@vue/test-utils": "~1.2.1",
    "@vue/vue2-jest": "^27.0.0-alpha.4",
    "axios-mock-adapter": "~1.19.0",
    "babel-jest": "~27.5.1",
    "codecov": "~4.0.0-0",
    "compression-webpack-plugin": "~9.2.0",
    "cypress": "^10.3.0",
    "eslint": "~8.14.0",
    "eslint-plugin-prettier": "~4.0.0",
    "eslint-plugin-vue": "~8.7.1",
    "prettier": "~2.6.2",
    "purgecss-webpack-plugin": "~4.1.3",
    "sass": "~1.50.1",
    "sass-loader": "~12.6.0",
    "terser-webpack-plugin": "~5.3.1",
    "ts-jest": "~27.1.4",
    "typescript": "~4.6.3",
    "vue-cli-plugin-vuetify": "~2.4.8",
    "vue-template-compiler": "~2.6.14",
    "vuetify-loader": "~1.7.2",
    "webpack": "~5.72.0",
    "webpack-bundle-analyzer": "~4.5.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier",
      "@vue/typescript"
    ],
    "rules": {
      "no-console": 0,
      "error": "off",
      "no-debugger": 0,
      "quotes": [
        "error",
        "single"
      ]
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "verbose": true,
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "preset": "@vue/cli-plugin-unit-jest/presets/typescript-and-babel",
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "@vue/vue2-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(showdown|vue-underscore|vue-ctk-date-time-picker|codemirror|vue-qrcode-reader|vue-qrcode)/)"
    ]
  },
  "engines": {
    "node": ">=16.14.2",
    "npm": ">=8.1.2"
  },
  "license": "MIT"
}
