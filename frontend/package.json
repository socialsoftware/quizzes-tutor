{
  "name": "tutor-frontend",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve",
    "build": "NODE_ENV=production vue-cli-service build --mode production",
    "build:dev": "NODE_ENV=development vue-cli-service build --mode development",
    "lint": "vue-cli-service lint",
    "prod": "vue-cli-service serve --mode production",
    "prettier": "prettier --write \"**/**/*.{js,css,scss,vue}\""
  },
  "engines": {
    "node": ">=10.16.0",
    "npm": ">=6.12.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.11.2",
    "axios": "^0.19.0",
    "material-design-icons-iconfont": "^5.0.1",
    "showdown": "^1.9.1",
    "typeface-roboto": "0.0.75",
    "vue": "^2.6.10",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.1.3",
    "vue-simplemde": "^1.0.0",
    "vue-underscore": "^0.1.4",
    "vuetify": "^2.1.11",
    "vuetify-datetime-picker": "^2.1.1",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@mdi/font": "^4.4.95",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-plugin-typescript": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/eslint-config-typescript": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "compression-webpack-plugin": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "fibers": "^3.1.1",
    "prettier": "^1.18.2",
    "sass": "^1.22.12",
    "sass-loader": "^7.3.1",
    "terser-webpack-plugin": "^2.2.1",
    "typescript": "3.5.3",
    "vue-cli-plugin-vuetify": "^0.6.3",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.2.2",
    "webpack-bundle-analyzer": "latest"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "prettier",
      "@vue/typescript"
    ],
    "rules": {
      "no-console": 0,
      "error": "off",
      "no-debugger": 0
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "plugins": [
      "prettier"
    ]
  }
}
